{
  "name": "@benmvp/cli",
  "version": "0.0.1",
  "description": "A highly-opinionated, zero-config CLI for consistent infra for Ben Ilegbodu's Typescript-based libraries",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "jsnext:main": "lib/esm/index.js",
  "types": "lib/index.d.ts",
  "bin": "./bin/benmvp",
  "repository": "https://github.com/benmvp/benmvp-cli.git",
  "author": "Ben Ilegbodu <ben@benmvp.com>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "babel src --out-dir lib/cjs --extensions '.ts,.js' --ignore 'src/**/*.spec.ts' --copy-files",
    "prestart": "yarn run build",
    "start": "./bin/benmvp start",
    "pretest": "yarn run build",
    "test": "./bin/benmvp test",
    "prebuild": "yarn run bootstrap",
    "build": "./bin/benmvp build --formats cjs esm type"
  },
  "engines": {
    "node": ">= 8"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.6.0",
    "bundlesize": "^0.17.0",
    "eslint": "^5.14.0",
    "eslint-config-eventbrite": "^5.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^23.6.0",
    "jest-runner-eslint": "^0.7.1",
    "jest-runner-tsc": "^1.3.2",
    "lodash": "^4.17.11",
    "typescript": "^3.2.1",
    "typescript-eslint-parser": "^21.0.2",
    "yargs": "^13.1.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^11.9.0",
    "@types/yargs": "^12.0.1"
  }
}
