    "babel-jest": "^23.6.0",
    "bundlesize": "^0.17.0",
    "jest": "^23.6.0",
    "jest-runner-tsc": "^1.3.2",
    "lodash": "^4.17.11",
    "typescript": "^3.2.1",
    "yargs": "^12.0.5"
  },
